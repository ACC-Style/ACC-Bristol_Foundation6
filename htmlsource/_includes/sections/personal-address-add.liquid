{% include elements/repeaterAdd-begin.liquid title="Address" %}

{% comment %} end of front{% endcomment %}
<div class="row">
    <div class="question column medium-8 large-6 ">
        <div class="label-holder">
            <label>
                <span class="text">
                    Address Type
                </span>
                <span class="required-holder">
                    <i class="fa fa-holder">
                    </i>
                </span>
            </label>
        </div>
        <div class="input-holder">
            <select id="addresstype" name="">
                <option value="">Work</option>
                <option value="">Home</option>
            </select>
        </div>
        <div class="message-holder">
            <div class="message-holder">
                <small class="" data-bind="css:{ error: countrycode.hasError, warning: countrycode.hasWarning }">
                    <i class="fa fa-holder">
                    </i>
                    <!-- ko text: countrycode.validationMessage -->
                    <!-- /ko -->
                </small>
            </div>
        </div>
    </div>
    <div class="question column medium-8 large-6 ">
        <div class="label-holder">
            <label>
                <span class="text">
                    Country
                </span>
                <span class="required-holder">
                    <i class="fa fa-holder">
                    </i>
                </span>
            </label>
        </div>
        <div class="input-holder">
            <select id="" name="">
                <option value="">
                </option>
            </select>
        </div>
        <div class="message-holder">
            <div class="message-holder">
                <small class="" data-bind="css:{ error: countrycode.hasError, warning: countrycode.hasWarning }">
                    <i class="fa fa-holder">
                    </i>
                    <!-- ko text: countrycode.validationMessage -->
                    <!-- /ko -->
                </small>
            </div>
        </div>
        <div class="hnt">
        </div>
    </div>
</div>
<div class="row" id="institution">
    {% include snippets/question-text.liquid label="Institution Name" size="small-12" state="required" hint="" %}
    
</div>
<div class="row">
    {% include elements/street-address.liquid %}
</div>
<div class="row">

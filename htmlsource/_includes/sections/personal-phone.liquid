
<div class="row" id="Phone">
    <div class="column">
        <h2>
            Phone Number(s)
            <div class="sub-line">
                <b>
                    At least one phone number is required.
                </b>
            </div>
        </h2>
        <div class="repeater">
            <!-- ko foreach: addressinfo() -->
            <div class="render animated fadeIn">
                <ul class="render-holder">
                    <li class="has-tip starred primary" data-bind="visible: isprimary" title="Preferred Address">
                        <i class="fa fa-star">
                        </i>
                        Preferred Phone Number
                    </li>
                    <li class="has-tip locked" data-bind="visible: isprimary" title="Locked: Primary Address - Please use edit to change">
                        <i class="fa fa-lock" title="Marked as Preferred">
                        </i>
                        This phone number can be edited but not deleted
                    </li>
                    <!--ko if: 'home' == 'work' -->
                    <!--/ko-->
                    <!--ko ifnot: 'home' == 'work' -->
                    <li class="loud" data-bind="text: address1">
                         <span class="strong">Work</span> <span class="italic break subtle" data-bind="text: countrycode">
                        USA
                    </span>202-555-5555
                    </li>
                    <!--/ko-->
                </ul>
                <div class="render-actions">
                    <a class="edit" data-bind="click: function() { $root.editCustomSection('address','addressinfo', $data) }" href="javascript:void(0)">
                        <i class="fa fa-holder">
                        </i>
                        <span class="text">
                            Edit
                        </span>
                    </a>
                    <a class="set disabled">
                        <i class="fa fa-star">
                        </i>
                        <span class="text">
                            mark as preferred
                        </span>
                    </a>
                    <a class="delete disabled" data-bind="visible: (!isprimary() && !preferredmailing() ),click: function() { $root.deleteCustomSection('address','addressinfo', $data) }" href="javascript:void(0)" >
                        <i class="fa fa-holder">
                        </i>
                        <span class="text">
                            Delete
                        </span>
                    </a>
                </div>
                <div class="message-holder">
                    <small class="warning">
                        <i class="fa fa-holder-dynamic">
                        </i>
                        This is a dynamic render message.
                    </small>
                </div>
            </div>
        </div>

    </div>
</div>




{% extends 'layouts/layout' %}
{% block content %}

<div class="page paper-page">
	<div class="">
		{% include "elements/page-message-missing-data" %}
	</div>
	<div class="page-wrapper">
		<div class="row">
			<h1 class="column"><i class="fa fa-user"></i> &nbsp; Profile
				<span class="sub-line">
					Your profile will be used to fill in information in your applications.  Please keep this information as up to date as possible to be considered for committees.
				</span>
			</h1>
		</div>

		<div class="row">
		
			<div id="contactData" class="column section-padding">
				<div class="card">
					<div class="colored-paper info">
						<i class="fa fa-envelope">&nbsp;</i><b class="no-vertical-margin">Contact Info</b>
					</div>
					<div class="section-padding row">
						<div class="column small-6 medium-4">
							<h5>Name: <span class="subheader">Zeta Gamma, FACC</span></h5>
							
						</div>
						<div class="column small-6 medium-4">
							<h5>Email:
							<span class="subheader">zgamma@gmail.com </span>
							</h5>
							<small><a href="" class="link"><i class="fa fa-pencil"></i> Edit on ACC.org</a></small>
						</div>
					</div>
				</div>
			</div>
			<div id="proExperianceData" class="column section-padding">
				<div class="card">
					<div class="colored-paper info">
						<i class="fa fa-envelope">&nbsp;</i><b class="no-vertical-margin">Professional Experiance</b>
					</div>
					<div class="section-padding row column">
						<div class="question">
							<div class="input-holder">
								<div class="input-space">
									<textarea name="" id="" cols="30" rows="10">This information is synced between here and any appliation you submit</textarea>
								</div>
							</div>
							<div class="hint">This information is synced between here and any appliation you submit</div>
						</div>
					</div>
				</div>
			</div>
			<div id="publicationsData"  class="column section-padding">
				<div class="card">
					<div class="colored-paper warning">
						<i class="fa fa-envelope">&nbsp;</i><b class="no-vertical-margin">Publications</b>
					</div>
					<div class="section-padding row column">
						<div class="question warning">
							<div class="input-holder">
								<div class="input-space">
									<textarea name="" id="" cols="30" rows="10">This information is synced between here and any appliation you submit</textarea>
								</div>
							</div>
							<div class="message-holder">
								<small class="warning"> <i class="fa fa-holder"></i>Missing Data</small>
							</div>
							<div class="hint">This information is synced between here and any appliation you submit</div>
						</div>
					</div>
				</div>
			</div>
			<div class="column section-padding">
				<div id="awardsData" class="card">
					<div class="colored-paper warning">
						<i class="fa fa-envelope">&nbsp;</i><b class="no-vertical-margin">Awards</b>
					</div>
					<div class="section-padding row column">
						<div class="question warning">
							<div class="input-holder">
								<div class="input-space">
									<textarea name="" id="" cols="30" rows="10">This information is synced between here and any appliation you submit</textarea>
								</div>
							</div>
							<div class="message-holder">
								<small class="warning"> <i class="fa fa-holder"></i>Missing Data</small>
							</div>
							<div class="hint">This information is synced between here and any appliation you submit</div>
						</div>
					</div>
				</div>
			</div>
			<div id="Compentancy"  class="column section-padding">
				<div class="card">
					<div class="colored-paper">
						<i class="fa fa-envelope">&nbsp;</i><b class="no-vertical-margin">Competency</b>
					</div>
					<div class="section-padding row">
						<div class="column small-6 medium-4">
							<h3>Leadership</h3>
							{% include "elements/slider" %}
						</div>
						<div class="column small-6 medium-4">
							<h3>Technical</h3>
							{% include "elements/slider" %}
						</div>
						<div class="column small-6 medium-4">
							<h3>Comp X</h3>
							{% include "elements/slider" %}
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="row section-padding ">
			<div class="column text-right">
				<a href="" class="button secondary">Cancel</a>
				<a href="" class="button">Save</a>
			</div>
		</div>
	</div>
</div>

{% endblock %}

{% comment %}<img src="output/abstract-q-c-640-480-1.jpg">{% endcomment %}
{% extends 'layouts/layout' %}
{% block content %}

<div class="page paper-page">
	<div class="page-wrapper">
		<div class="row">
			<h1 class="column"><i class="fa fa-user"></i> &nbsp; Profile</h1></div>
		<div class="row">
				<div class="page-message callout warning no-margin">
				  <a class="close-bar">
				    <span class="close-button">
				      <i class="fa fa-times" aria-hidden="true"></i>
				    </span>
				  </a>
				  <div class="message-container">
				    <div class="icon-holder">
				      <span class="fa-stack fa-lg">
				        <i class="fa fa-square-o fa-stack-2x"></i>
				        <i class="fa fa-holder-dynamic fa-stack-1x"></i>
				      </span>
				    </div>
				    <div class="message-holder">
				      <h2>Your Profile has missing Data</h2>
				      <p>Your profile must be complete and up to date to apply for any committees</p>
				    </div>
				  </div>
				</div>

		</div>
		<div class="row">
		
			<div class="column section-padding">
				<div class="card">
					<div class="colored-paper info">
						<i class="fa fa-envelope">&nbsp;</i><b class="no-vertical-margin">Contact Info</b>
					</div>
					<div class="section-padding row">
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
					</div>
				</div>
			</div>
			<div class="column section-padding">
				<div class="card">
					<div class="colored-paper info">
						<i class="fa fa-envelope">&nbsp;</i><b class="no-vertical-margin">Professional Experiance</b>
					</div>
					<div class="section-padding row">
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
					</div>
				</div>
			</div>
			<div class="column section-padding">
				<div class="card">
					<div class="colored-paper warning">
						<i class="fa fa-envelope">&nbsp;</i><b class="no-vertical-margin">Publications</b>
					</div>
					<div class="section-padding row">
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
					</div>
				</div>
			</div>
			<div class="column section-padding">
				<div class="card">
					<div class="colored-paper warning">
						<i class="fa fa-envelope">&nbsp;</i><b class="no-vertical-margin">Awards</b>
					</div>
					<div class="section-padding row">
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
						<div class="column small-6 medium-4">
							{% include "elements/generic-question" %}
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="row section-padding ">
			<div class="column text-right">
				<a href="" class="button secondary">Cancel</a>
				<a href="" class="button">Save</a>
			</div>
		</div>
	</div>
</div>
<style>
	.card.ghost h6 a.button{maring-top:-.5rem;}
	.card.ghost + .card.ghost{ margin-top:.25rem; }
	.single-line-height{line-height: 1;}
</style>
{% endblock %}


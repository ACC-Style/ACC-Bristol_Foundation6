{% extends 'layouts/admin-layout' %}
{% block content %}
<div class="page paper-page">
    <div class="section-nav back">
        <a href="admin-applications.html">
            <div class="circle">
                <i class="fa fa-holder-dynamic">
                </i>
            </div>
            <div class="text">
                Back
            </div>
        </a>
    </div>
    <div class="page-wrapper">
        <div class="row position-relative">
            <div class="column">
                <h1>
                    Doctor Dove Special Dark
                    <a class="button secondary float-right" href="">
                        <i class="fa fa-envelope">
                        </i>
                        Email Applicant
                    </a>
                    <div class="sub-line">
                        <small class="label warning">
                            Status: Sent for Revision
                        </small>
                    </div>
                </h1>
            </div>
        </div>
        {% comment %}
        <nav class="overlap nav">
            <ul class="tabs ">
                <li class="tab-title">
                    <a class="" href="admin-committees-details.html">
                        Interested Committees
                    </a>
                </li>
                <li class="tab-title is-active">
                    <a href="admin-committees.html">
                        Digital CV
                    </a>
                </li>
                <li class="tab-title">
                    <a href="admin-committees.html">
                        Letter of Refference
                    </a>
                </li>
            </ul>
        </nav>
        {% endcomment %}
        <div class="row">
            <div class="column medium-6">
                <div class="card">
                    <div class="colored-paper">
                        <h2>
                            Interested Activities/Committees
                        </h2>
                    </div>
                    <div class="section-padding">
                        <div class="accordion">
                            {% include "admin/sections/interested-committee-list" %}
                            <h3>
                                Disclosures
                            </h3>
                            <span class="label success radius">
                                <i class="fa fa-check">
                                </i>
                                Up to date
                            </span>
                            <a href="">
                                <i class="fa fa-file">
                                </i>
                                View Disclosures
                            </a>
                        </div>
                    </div>
                </div>
                <div class="row column section-padding">
                    <a class="link" href="">
                        Show Full list of Activites/Committees
                    </a>
                </div>
            </div>
            <div class="column medium-6">
                <div class="card">
                    <div class="colored-paper">
                        <h2>
                            Statement of Reference
                        </h2>
                    </div>
                    <div class="section-padding">
                        <h3>
                            Abe Lincoln, FACC
                            <div class="sub-line">
                                Institution Name: Gettysburg Research
                            </div>
                        </h3>
                        <p>
                            {% include "elements/lorem" %}
                        </p>
                    </div>
                </div>
                <div class="section-padding">
                    <div class="card">
                        <div class="colored-paper">
                            <h2>
                                Digital CV
                            </h2>
                        </div>
                        <div id="Application-Details">
                            <div class="section-padding">
                                <h3>
                                    Professional Experiance
                                </h3>
                                <p>

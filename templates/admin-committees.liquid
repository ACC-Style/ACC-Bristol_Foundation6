{% extends 'layouts/admin-committee-layout' %}
{% block content %}
<div class="row page-header section-padding">
    <div class="column">
        <h1 >
           <a href="admin-committees-dashboard.html"><i class="fa fa-th-list" aria-hidden="true"></i></a> <i class="fa fa-angle-right" aria-hidden="true"></i> <a href="admin-committees-list.html" class="link">Commitee Name ABC</a> <i class="fa fa-angle-right" aria-hidden="true"></i> <a href="admin-committees-list.html" class="link">Applicant List</a>
           <i class="fa fa-angle-right" aria-hidden="true"></i> Applicant Name
           
        </h1>
        <div>
            <a class="button tiny radius hollow" href="admin-committees-list.html">Applicant List</a>
            <a class="button tiny radius " href="http://localhost:8000/docs/admin-committees.html">Applicant Details</a>
        </div>
    </div>
</div>

<section class="row collapse">
    <aside class="column small-hidden medium-4 large-3">
        <div class="colored-paper no-margin ">
            <h4 class="no-vertical-margin single-line-height">
                Elegible
            </h4>
        </div>
        <div class="">
            <nav class="nav-list">
                <ul>
                    <li class="selected">
                        <a href="">
                            <span class="text">
                                <span class="name">
                                    Doctor Beta
                                </span>
                                <span>
                                    FACC
                                </span>
                            </span>
                            <i class="fa fa-arrow-circle-right">
                            </i>
                        </a>
                    </li>
                    <li>
                        {% include 'elements/nominee' %}
                    </li>
                </ul>
            </nav>
        </div>
        <div class="colored-paper no-margin warning">
            <h4 class="no-vertical-margin single-line-height">
                <i class="fa fa-question-circle">
                </i>
                Unset Elegility
            </h4>
        </div>
        <div class="">
            <nav class="nav-list">
                <ul>
                    <li>
                        {% include 'elements/nominee' %}
                    </li>
                    <li>
                        {% include 'elements/nominee' %}
                    </li>
                </ul>
            </nav>
        </div>
        <div class="content-padding text-right">
            <a class="link" href="">
             <i class="fa fa-eye"></i>   show inelegible members
            </a>
        </div>
    </aside>
    <article class="layer-down column medium-8 large-9">
        <div class="page-wrapper content-padding" style="max-width: 100%">
            <div class="row column">
                <div class="card">
                    <div class="colored-paper">
                        <h2 class="single-line-height no-vertical-margin">
                            <span class="float-right">
                                <a href="" class="button success small hide-for-large">Add Comment</a>
                            </span>
                            <span class="float-right">
                                
                                <select id="" name="">
                                    <option default="" value="">
                                        Elegible
                                    </option>
                                    <option value="">
                                        InElegible
                                    </option>
                                    <option value="">
                                        Unknown
                                    </option><option value="">
                                        Appoint
                                    </option>
                                </select>
                            </span>
                            Doctor Beta, FACC
                            <small class="subheader">
                                ( Average Rating: 2.4
                                <i class="fa fa-star">
                                </i>
                                )
                            </small>
                            <span class="sub-line">
                                email@email.com
                            </span>
                        </h2>
                    </div>
                    <div class="row section-padding">
                        <div class="column large-6">
                            <div class="callout highlighted">
                                <h3>
                                    Statement of Interest
                                </h3>
                                <p>
                                    {% include 'elements/lorem' %}
                                </p>
                            </div>
                            <div class="callout info">
                                <h3>
                                    Competency
                                </h3>
                                <div class="row">
                                    <div class="column">
                                        <ul class="compency-list">
                                            <li>
                                                Exhibits Influential Leadership
                                            </li>
                                            <li>
                                                Demonstrates Strategic Leadership
                                            </li>
                                            <li>
                                                Maintains Organizational Awareness & Stewardship
                                            </li>
                                        </ul>
                                        <p>
                                            {% include 'elements/lorem' %}
                                        </p>
                                    </div>
                                </div>
                            </div>
                            <div class="secion-padding content-padding">
                                <h3>
                                    Committee Ranking
                                </h3>
                                <nav class="nav-list nav-checklist">
                                    <ul>
                                        <li class="selected">
                                            <b>
                                                1
                                            </b>
                                            Committee ABC
                                        </li>
                                        <li class="disabled">
                                            <b>
                                                2
                                            </b>
                                            ~Committee Outside Perview ~
                                        </li>
                                        <li class="">
                                            <b>
                                                3
                                            </b>
                                            Committee FGH
                                        </li>
                                        <li class="">
                                            <b>
                                                4
                                            </b>
                                            Committee XYZ
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                        <div class="column large-6">
                            <div class="seciton-padding callout">
                                <h2>
                                    Comment
                                </h2>
                                <div class="colored-paper disabled row">
                                    <div class="switch-input-space column small-12 no-padding">
                                        <label class="switch-value" for="checkbox-value-switch-round" style="width:1%;">
                                            Attach Comment To
                                        </label>
                                        <div class="switch value-switch switch-4x radius" style="width:2%;">
                                            <input class="switch-input" id="checkbox-value-switch-round" type="checkbox" value="false">
                                                <label class="switch-paddle" for="checkbox-value-switch-round">
                                                    <span class="show-for-sr">
                                                        Do you like me?
                                                    </span>
                                                    <span aria-hidden="true" class="switch-active">
                                                        Committee
                                                    </span>
                                                    <span aria-hidden="true" class="switch-inactive">
                                                        Global
                                                    </span>
                                                </label>
                                            </input>
                                        </div>
                                    </div>
                                </div>
                                <div class="group-divide-bottom section-padding disabled">
                                    <h3>
                                        Rating
                                    </h3>
                                    {% include 'elements/stars-buttons'%}
                                </div>
                                <div class="group-divide-bottom section-padding row column">
                                    <div class="switch-input-space column small-12">
                                        <div class="switch info-active success-inactive switch-2x">
                                            <input class="switch-input" id="active-inactive" name="exampleSwitch" type="checkbox">
                                                <label class="switch-paddle" for="active-inactive">
                                                    <span class="show-for-sr">
                                                        Do you like me?
                                                    </span>
                                                    <span aria-hidden="true" class="switch-active">
                                                        No
                                                    </span>
                                                    <span aria-hidden="true" class="switch-inactive">
                                                        Yes
                                                    </span>
                                                </label>
                                            </input>
                                        </div>
                                        <label class="switch-value" for="active-inactive">
                                            Chair Pick
                                        </label>
                                    </div>
                                </div>
                                <div class="section-padding row column">
                                    <textarea id="" name="" rows="5" style="margin-bottom: 0">
                                    </textarea>
                                </div>
                                <a class="button float-right" href="">
                                    Add Comment
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="colored-paper info">
                        <nav>
                            <ul class="nav tabs">
                                <li class=" selected ">
                                    <a>
                                        Applicaiton
                                    </a>
                                </li>
                                <li>
                                    <a href="">
                                        Appointments
                                    </a>
                                </li>
                                <li>
                                    <a href="">
                                        Chapter
                                    </a>
                                </li>
                                <li>
                                    <a href="">
                                        Committees
                                    </a>
                                </li>
                                <li>
                                    <a href="">
                                        Sections
                                    </a>
                                </li>
                                <li>
                                    <a href="">
                                        Attendance
                                    </a>
                                </li>
                            </ul>
                        </nav>
                    </div>
                    <div class="row">
                        <div class="columns large-6">
                            <h2 class="with-underline show-for-large">
                                Application Details
                            </h2>
                            {% include "admin/sections/com/application-details"%}
                            <h3>
                                Disclosures
                            </h3>
                            <span class="label success radius">
                                <i class="fa fa-check">
                                </i>
                                Up to date
                            </span>
                            <a href="">
                                <i class="fa fa-file">
                                </i>
                                View Disclosures
                            </a>
                        </div>
                        <div class="columns large-6 show-for-large">
                            <h2 class="with-underline show-for-large">
                                Comments & Ratings
                                <div class="float-right">
                                    2.4
                                    <i class="fa fa-star">
                                    </i>
                                </div>
                            </h2>
                            {% include "admin/sections/com/application-comments"%}
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <button aria-controls="offCanvasLeftSplit" aria-expanded="false" class="button warning small circle-button" data-toggle="offCanvasLeftSplit" id="member-toggle" type="button">
            <i class="fa fa-bars" style="color:white">
            </i>
        </button>
    </article>
</section>
{% endblock %}


{% extends 'layouts/admin-committee-layout' %}
{% block content %}
        <div class="row ">
            <div class="column">
                <h1 class="no-vertical-margin">
                    Committee Name ABC

                    <a href="" class="float-right secondary button hollow"><i class="fa fa-pencil"></i> Edit Committee Details</a>
                </h1>
            </div>
        </div>
    </div>
    <nav class="overlap nav">
        <ul class="tabs ">
            <li class="tab-title">
                <a class="home" href="admin-committees-details.html">
                    <i class="fa fa-holder">
                    </i>
                    Committee Details
                </a>
            </li>
            <li class="tab-title is-active">
                <a href="admin-committees.html">
                    Current Nominees
                </a>
            </li>
            <li class="tab-title disabled">
                <a href="admin-committees.html">
                    Appointment
                </a>
            </li>
        </ul>
    </nav>
    <div class="page-wrapper">
        <div class="row column ">
            <h2 class="">
                Current Nominees
                {% comment %}<div class="float-right button-group tiny no-vertical-margin">
                    <a class=" secondary button" href="">
                        Report
                    </a>
                </div>{% endcomment %}
            </h2>
        </div>
    </div>
</div>
<div class="mobile-offcanvas-wrapper" style="border-top:1px solid grey">
    <section class="row collapse">
        <aside class="column medium-3">
            <div class="">
                <div class="colored-paper no-margin ">
                    <h4 class="no-vertical-margin single-line-height">
                        Elegible
                    </h4>
                </div>
                <div class="section-padding">
                    <nav class="nav nav-list">
                        <ul>
                            <li class="selected">
                                <a href="">
                                    <span class="text">
                                        <span class="name">
                                            Doctor M&amp;M
                                        </span>
                                        <span>
                                            FACC
                                        </span>
                                    </span>
                                    <i class="fa fa-arrow-right">
                                    </i>
                                </a>
                            </li>
                            <li>
                                {% include 'elements/nominee' %}
                            </li>
                        </ul>
                    </nav>
                   <div class="content-padding">
                        <a class="small expanded secondary button" href="">
                        Nominate Member Not In List
                    </a>
                   </div>
                </div>
                <div class="colored-paper no-margin warning">
                    <h4 class="no-vertical-margin single-line-height">
                        <i class="fa fa-question-circle">
                        </i>
                        Unknown Elegiblity
                    </h4>
                </div>
                <div class="section-padding">
                    <nav class="nav nav-list">
                        <ul>
                            <li>
                                {% include 'elements/nominee' %}
                            </li>
                            <li>
                                {% include 'elements/nominee' %}
                            </li>
                        </ul>
                    </nav>
                </div>
                <div class="colored-paper no-margin disabled">
                    <h4 class="no-vertical-margin single-line-height">
                        Inelegible
                    </h4>
                </div>
                <div class="section-padding content-padding">
                    <a class="link" href="">
                        view inelegible members
                    </a>
                </div>
            </div>
        </aside>
        <article class="column medium-9 page paper-page" style="background:url('Assets/img/textured_paper.png') repeat, url('Assets/img/bottom-shade.png') bottom repeat-x, url('Assets/img/top-shade.png') top repeat-x">
            <div class="page-wrapper content-padding" style="max-width: 100%">
                <div class="row column">
                    <div class="card">
                        <div class="colored-paper">
                            <h2 class="single-line-height no-vertical-margin">
                                <span class="float-right">
                                    <select id="" name="">
                                        <option default="" value="">
                                            Elegible
                                        </option>
                                        <option value="">
                                            InElegible
                                        </option>
                                        <option value="">
                                            Unknown
                                        </option>
                                    </select>
                                </span>
                                Doctor Beta, FACC
                                 <small class="subheader">
                                  ( Average Rating: 2.4 <i class="fa fa-star"></i> )
                                </small>
                                <span class="sub-line">
                                    email@email.com
                                </span>
                            </h2>
                        </div>
                        <div class="row section-padding">
                            <div class="column medium-6">
                                <div class="section-padding content-padding callout highlighted">
                                    <h3>
                                        Statement of Interest
                                    </h3>
                                    <p>
                                        {% include 'elements/lorem' %}
                                    </p>
                                </div>
                                <div class="section-padding content-padding callout info">
                                    <h3>
                                        Competency
                                    </h3>
                                    <div class="row">
                                       <div class="column">
                                        <ul class="compency-list">
                                            <li>Exhibits Influential Leadership</li>
                                            <li>Demonstrates Strategic Leadership</li>
                                            <li>Maintains Organizational Awareness & Stewardship</li>
                                        </ul>

                                        <p>
                                        {% include 'elements/lorem' %}
                                        </p>
                                       </div>
                                    </div>
                                </div>
                                <div class="secion-padding content-padding">
                                    <h3>
                                        Committee Ranking
                                        
                                    </h3>
                                    <nav class="nav nav-list nav-checklist">
                                        <ul>
                                            <li class="selected">
                                                <b>
                                                    1
                                                </b>
                                                Committee ABC
                                            </li>
                                            <li class="disabled">
                                                <b>
                                                    2
                                                </b>
                                                ~Committee Outside Perview ~
                                            </li>
                                            <li class="">
                                                <b>
                                                    3
                                                </b>
                                                Committee FGH
                                            </li>
                                            <li class="">
                                                <b>
                                                    4
                                                </b>
                                                Committee XYZ
                                            </li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                            <div class="column medium-6">
                                <div class="seciton-padding callout">
                                    <h2 class="with-underline">Comment</h2>
                                    
                                    <div class="group-divide-bottom section-padding">
                                    <h3>
                                        Rating
                                    </h3>
                                        {% include 'elements/stars-buttons'%}
                                    </div>
                                    
                                    <div class="group-divide-bottom section-padding row column">
                                        <div class="switch-input-space column small-12">
                                          <div class="switch info-active success-inactive switch-2x">
                                            <input class="switch-input" id="active-inactive" type="checkbox" name="exampleSwitch">
                                            <label class="switch-paddle" for="active-inactive">
                                              <span class="show-for-sr">Do you like me?</span>
                                              <span class="switch-active" aria-hidden="true">No</span>
                                              <span class="switch-inactive" aria-hidden="true">Yes</span>
                                            </label>
                                          </div>
                                          <label class="switch-value" for="active-inactive">Chair Pick</label>
                                        </div>
                                    </div>
                                    <div class="section-padding row column">
                                    <textarea id="" name="" rows="5" style="margin-bottom: 0"></textarea>
                                    <div class="switch-input-space column small-12 colored-paper disabled" style="float: none;margin: 0;padding: .25em;">                                    <label for="checkbox-value-switch-round" class="switch-value" style="width:1%;"> Attach Comment To</label>
                                      <div class="switch value-switch switch-4x tiny round" style="width:2%;">
                                        <input class="switch-input" id="checkbox-value-switch-round" type="checkbox" value="false">
                                        <label class="switch-paddle" for="checkbox-value-switch-round">
                                          <span class="show-for-sr">Do you like me?</span>
                                          <span class="switch-active" aria-hidden="true">Global</span>
                                          <span class="switch-inactive" aria-hidden="true">Committee</span>
                                        </label>
                                      </div>
                                    </div>
                                    </div>
                                    
                                    <a class="button float-right" href="">
                                        Add Comment
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="colored-paper info">
                            <nav>
                                <ul class="nav tabs">
                                    <li class=" selected ">
                                        <a>
                                            Applicaiton
                                        </a>
                                    </li>
                                    <li><a href="">Appointments</a></li>
                                    <li><a href="">Chapter</a></li>
                                    <li><a href="">Committees</a></li>
                                    <li><a href="">Sections</a></li>
                                    <li><a href="">Attendance</a></li>
                                </ul>
                            </nav>
                        </div>
                        <div class="row">
                            <div class="columns large-6">
                            <h2 class="with-underline show-for-large">Application Details</h2>
                            {% include "admin/sections/com/application-details"%}
                            <h3>Disclosures</h3>
                            <span class="label success radius"><i class="fa fa-check"></i> Up to date</span> &nbsp;
                            <a href=""><i class="fa fa-file"></i>&nbsp;View Disclosures</a>
                            </div>
                            <div class="columns large-6 show-for-large">
                            <h2 class="with-underline show-for-large">
                            Comments &amp; Ratings
                                <div class="float-right">
                                    2.4 <i class="fa fa-star"></i>
                                </div>
                            </h2>
                            {% include "admin/sections/com/application-comments"%}</div>
                        </div>
                        
                    </div>
                </div>
            </div>
            <div >
                <div class="column">
                    <div class="button-group small float-right">
                        <a class="button secondary" href="">
                            Remove
                        </a>
                        <a class="button" href="">
                            Award Nomination
                        </a>
                    </div>
                </div>
            </div>
        </article>
    </section>
</div>
{% endblock %}


<!doctype html>
<html class="no-js" lang="en">
<head>
  <title>My Style Guide</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="assets/css/styledown.css">
  <link rel="stylesheet" href="assets/css/normalize.min.css">
  {% comment %}<link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,700italic,700,600,400,300|Roboto+Slab:400,700|Raleway:300">{% endcomment %}
  <link rel="stylesheet" href="assets/css/jquery-ui.css">
  <link rel="stylesheet" href="assets/css/jquery-uistructure.css">
  <link rel="stylesheet" href="assets/css/foundation-sites.css">
   <link rel="stylesheet" href="assets/css/font-awesome.css">
  <link rel="stylesheet" href="assets/css/index.css">

</head>
	<body>
    <a class="hide-for-medium" onClick="$('.mobile-offcanvas-wrapper .split-layout').toggleClass('navOn')" style="display: block;width: 100%;text-align: center;color: white;background-color: #00386b;"><i class="fa fa-bars"></i> Menu</a>
    {% include 'sections/header' %}

    <div class="mobile-offcanvas-wrapper">
      <section class="split-layout fill-page-height medium navOn">
        <aside class="split-sidebar">
         {% include "sidebar" %}
        </aside>
        <article class="split-content"  data-bind="page: {id:'content', role: 'start'}">
          <div class="page-wrapper">
            <div class="content-padding">
              {% include "pages/introduction" %}
              {% include "pages/framework" %}
              {% include "pages/branding" %}
              {% include "pages/typography" %}
              {% include "pages/tables" %}
              {% include "pages/buttons" %}
              {% include "pages/navigation" %}
              {% include "pages/qna" %}
              {% include "pages/layouts" %}  
              {% include "pages/paper" %}   
              {% include "pages/space-savers" %}                            
            </div>
          </div>
        </article>
      </section>
    </div>
    {% include 'sections/footer' %}
    {% include "scripts" %}
	</body>
</html>
{% include "head" %}
{% include 'sections/header-active-application' %}
<div class="mobile-offcanvas-wrapper" data-bind="page: {id:'sections', role: 'start'}">
    <div class="paper-page page">
        <div class="page-header clearfix">
            <h1>
                {% block page-name %}{% endblock %}
            </h1>
        </div>
        <div class="row collapse">
            <div class="column large-3 inset-container">
                <aside class="inset-area">
                    <div class="colored-paper" style="background-color: #444;">
                        <h2 class="clearfix text-center">
                            Open Committees
                        </h2>
                        <p class="text-center">
                            Select up to six committees to add your "Committees of Interest" list.
                        </p>
                        <a class="expanded hollow button tiny reverse" href="open-committees.html" style="">
                            Explore All Open Committees
                            <i class="fa fa-arrow-right">
                            </i>
                        </a>
                    </div>
                    {% comment %}{% include "elements/searchbar" %}{% endcomment %}
                    <div class="colored-paper clearfix text-center" style="background-color:#ccc;padding: .5em 0 0 0;">
{% comment %}                        <div class="switch-input-space column small-12">
                            <div class="switch value-switch switch-4x round">
                                <input class="switch-input" id="checkbox-value-switch-round" type="checkbox" value="false">
                                    <label class="switch-paddle" for="checkbox-value-switch-round">
                                        <span class="show-for-sr">
                                            Do you like me?
                                        </span>
                                        <span aria-hidden="true" class="switch-active">
                                            All
                                        </span>
                                        <span aria-hidden="true" class="switch-inactive">
                                            Nominated
                                        </span>
                                    </label>
                                </input>
                            </div>
                        </div>{% endcomment %}
                       <div class="content-padding">
                            <div class="button-group  selectable selectable-switch small">
                            <a href="" class="button  selected"><i class="fa fa-holder"></i>All</a>
                            <a href="" class="button "><i class="fa fa-holder"></i>Nominated</a>
                            <a href="" class="button "><i class="fa fa-holder"></i>Interested</a>
                        </div>
                       </div>
                    </div>
                    <div class="section-padding">
                        <ul class="committee-list">
                            <li class="committee-list-item selected checked">
                                <div class="committee-data">
                                    <div class="action">
                                        <span aria-controls="committee-data-3" aria-expanded="false" aria-haspopup="true" class="fa-stack fa-lg" data-is-focus="false" data-toggle="committee-data-3" data-yeti-box="committee-data-3">
                                            <i class="fa fa-circle fa-stack-2x">
                                            </i>
                                            <i class="fa fa-check fa-stack-1x">
                                            </i>
                                        </span>
                                    </div>
                                    <div class="committee">
                                        <h2 class="name">
                                            Assembly of Interantional Governors Steering Committee
                                            <div class="sub-line">
                                                ACC Guidance Committee
                                            </div>
                                        </h2>
                                        <span class="label secondary">
                                            Open Positions:
                                            <b>
                                                4
                                            </b>
                                        </span>
                                    </div>
                                </div>
                            </li>
                            <li class="committee-list-item selected checked">
                                <div class="committee-data">
                                    <div class="action">
                                        <span aria-controls="committee-data-3" aria-expanded="false" aria-haspopup="true" class="fa-stack fa-lg" data-is-focus="false" data-toggle="committee-data-3" data-yeti-box="committee-data-3">
                                            <i class="fa fa-circle fa-stack-2x">
                                            </i>
                                            <i class="fa fa-check fa-stack-1x">
                                            </i>
                                        </span>
                                    </div>
                                    <div class="committee">
                                        <h2 class="name">
                                            Assembly of Interantional Governors Steering Committee
                                            <div class="sub-line">
                                                ACC Guidance Committee
                                            </div>
                                        </h2>
                                        <span aria-haspopup="true" class="label has-tip" data-disable-hover="false" data-tooltip="" tabindex="1" title="Dr. Albert Einstien">
                                            Nominated
                                            <i class="fa fa-info-circle">
                                            </i>
                                        </span>
                                        <span class="label secondary">
                                            Open Positions:
                                            <b>
                                                4
                                            </b>
                                        </span>
                                    </div>
                                </div>
                            </li>
                            <li class="committee-list-item">
                                <div class="committee-data">
                                    <div class="action">
                                        <span aria-controls="committee-data-1" aria-expanded="false" aria-haspopup="true" class="fa-stack fa-lg" data-is-focus="false" data-toggle="committee-data-1" data-yeti-box="committee-data-1">
                                            <i class="fa fa-circle fa-stack-2x">
                                            </i>
                                            <i class="fa fa-check fa-stack-1x">
                                            </i>
                                        </span>
                                    </div>
                                    <div class="committee">
                                        <h2 class="name">
                                            The Chocolate Factory
                                            <div class="sub-line">
                                                ACC Guidance Committee
                                            </div>
                                        </h2>
                                        <span class="label secondary">
                                            Open Positions:
                                            <b>
                                                4
                                            </b>
                                        </span>
                                    </div>
                                </div>
                            </li>
                            <li class="committee-list-item selected checked">
                                <div class="committee-data">
                                    <div class="action">
                                        <span aria-controls="committee-data-2" aria-expanded="false" aria-haspopup="true" class="fa-stack fa-lg" data-is-focus="false" data-toggle="committee-data-2" data-yeti-box="committee-data-2">
                                            <i class="fa fa-circle fa-stack-2x">
                                            </i>
                                            <i class="fa fa-check fa-stack-1x">
                                            </i>
                                        </span>
                                    </div>
                                    <div class="committee">
                                        <h2 class="name">
                                            Candy Bar Wrappers
                                            <div class="sub-line">
                                                ACC Guidance Committee
                                            </div>
                                        </h2>
                                        <span class="label secondary">
                                            Open Positions:
                                            <b>
                                                4
                                            </b>
                                        </span>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                    <div style="position: static;left: 0;right: 0;bottom: 0;">
                        {% include "elements/card-pagination-bottom" %}
                    </div>
                </aside>
            </div>
            <div class="column large-3 inset-container">
                <div class=" inset-area " data-sticky-container>
               <div  class="sticky" data-sticky data-top-anchor="StatementofInterest:top" data-btm-anchor="disclosures">{% block active-application %}{% endblock %}</div>
                    
                </div>
            </div>
            <div class="column large-6">
                <div class="content-padding">
                    {% block content %}{% endblock %}
                </div>
            </div>
        </div>
    </div>
</div>
{% include 'sections/web-footer' %}
    {% include "scripts" %}
<div class="reveal large no-padding paper-page page" data-reveal="" id="comm-modal">
    {% include "sections/committee-overview" %}
    <button aria-label="Close modal" class="close-button" data-close="" type="button">
        <span aria-hidden="true">
            <i class="fa fa-times">
            </i>
        </span>
    </button>
</div>

                                            <label class="switch-paddle" for="checkbox-value-switch-round">
                                                <span class="show-for-sr">
                                                    Do you like me?
                                                </span>
                                                <span aria-hidden="true" class="switch-active">
                                                    All
                                                </span>
                                                <span aria-hidden="true" class="switch-inactive">
                                                    Nominated
                                                </span>
                                            </label>
                                        </input>
                                    </div>
                                </div>
                            </div>
                <div class="row collapse">
                    <div class="column large-3 inset-container collapse">
                        <aside class="inset-area ">

                            {% comment %}{% include "elements/searchbar" %}{% endcomment %}
                           
                            <div class="section-padding">
                                    <ul class="comittee-list">
                                        {% include "elements/comittee-list-item-notactive" %}
                                        <li class="committee--list-item selected">
                                            <div class="committee--data">
                                                <div class="committee">

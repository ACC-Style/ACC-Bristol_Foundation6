<div class="top-bar">
    <div class="hide-for-large" id="mobileMenu">
        <div class="top-bar-left" onclick="$('#siteMenu .top-bar-left').toggleClass('display-block')">
            <a class="top-bar-toggle">
                <i class="fa fa-bars">
                </i>
                Menu
            </a>
        </div>
        <div class="top-bar-right secondary" onclick="$('#siteMenu .search-bar').toggleClass('display-block')">
            <a class="top-bar-toggle small condensable">
                <span class="condense">
                    Committee Search
                </span>
                <i class="fa fa-search">
                </i>
            </a>
        </div>
        <div class="top-bar-right " onclick="$('#siteMenu .top-bar-right.user-nav').toggleClass('display-block')">
            <a class="top-bar-toggle small condensable">
                <span class="condense">
                    Profile
                </span>
                <i class="fa fa-user">
                </i>
            </a>
        </div>
    </div>
    <div class="stack-for-medium " id="siteMenu">
        <div class="top-bar-left show-for-large">
            <ul class="menu">

                <li>
                    <a href="#">
                        Dashboard
                    </a>
                </li>
                <li>
                    <a href="#">
                        Open Committees
                    </a>
                </li>
                <li>
                    <a href="#">
                        Requests
                    </a>
                </li>
            </ul>
        </div>
        <div class="top-bar-right secondary show-for-large" onclick="$('#siteMenu .search-bar').toggleClass('display-block')">
            <ul class="menu">
                <li>
                    <a class="top-bar-toggle small condensable">
                        <span class="condense">
                            Committee Search
                        </span>
                        <i class="fa fa-search">
                        </i>
                    </a>
                </li>
            </ul>
        </div>
        <div class="top-bar-right user-nav show-for-large">
            <ul class="menu">
                <li>
                    <a href="">
                      <i class="fa fa-user" aria-hidden="true"></i>  Profile
                    </a>
                </li>
                <li>
                    <a href="#">
                       <i class="fa fa-files-o" aria-hidden="true"></i> Publications
                    </a>
                </li>
                <li>
                    <a href="#">
                       <i class="fa fa-trophy" aria-hidden="true"></i> Awards 
                    </a>
                </li>
            </ul>
        </div>
        <div class="search-bar">
            <div class="question">
                <div class="input-holder">
                    <div class="input-space column small-6 medium-8 large-9 ">
                        <input name="" type="text">
                        </input>
                    </div>
                    <div class="input-space column small-6 medium-4 large-3 ">
                        <button class="button expanded">
                            Committee Search
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



<div class="page paper-page repeater-area content-padding" id="addAddress" style="display: none;">
    <div class="colored-paper success">
        <div class="content-padding row">
            <h4 class="no-vertical-margin" style="position: relative;">
                <i class="fa fa-plus">
                </i>
                Add Address
                <a class="close-button" href="" onclick="addressSlide(event)" style="color:#fff;">
                    <i class="fa fa-times">
                    </i>
                    Close
                </a>
            </h4>
        </div>
    </div>
    <div class="page-wrapper">
        <div class="row">
            <div class="column">
                <div class="card">
                    <div class="question-group section-padding success">
                        <!-- ko with: addressinfoForm -->
                        <div class="row">
                            <div class="question column medium-8 large-6 ">
                                <div class="label-holder">
                                    <label>
                                        <span class="text">
                                            Address Type
                                        </span>
                                        <span class="required-holder">
                                            <i class="fa fa-holder-dynamic">
                                            </i>
                                        </span>
                                    </label>
                                </div>
                                <div class="input-holder">
                                    <select id="" name="">
                                        <option value="">
                                            Choose...
                                        </option>
                                    </select>
                                </div>
                                <div class="message-holder">
                                    <div class="message-holder">
                                        <small class="" data-bind="css:{ error: countrycode.hasError, warning: countrycode.hasWarning }">
                                            <i class="fa fa-holder">
                                            </i>
                                            <!-- ko text: countrycode.validationMessage -->
                                            <!-- /ko -->
                                        </small>
                                    </div>
                                </div>
                            </div>
                            <div class="question column medium-8 large-6 ">
                                <div class="label-holder">
                                    <label>
                                        <span class="text">
                                            Country
                                        </span>
                                        <span class="required-holder">
                                            <i class="fa fa-holder-dynamic">
                                            </i>
                                        </span>
                                    </label>
                                </div>
                                <div class="input-holder">
                                    <select id="" name="">
                                        <option value="">
                                        </option>
                                    </select>
                                </div>
                                <div class="message-holder">
                                    <div class="message-holder">
                                        <small class="" data-bind="css:{ error: countrycode.hasError, warning: countrycode.hasWarning }">
                                            <i class="fa fa-holder">
                                            </i>
                                            <!-- ko text: countrycode.validationMessage -->
                                            <!-- /ko -->
                                        </small>
                                    </div>
                                </div>
                                <div class="hnt">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="question column large-4 " data-bind="css: { error: address1.hasError, required: address1.isRequired, warning: address1.hasWarning }">
                                <div class="label-holder">
                                    <label>
                                        <span class="text">
                                            Street Address 1
                                        </span>
                                        <span class="required-holder">
                                            <i class="fa fa-holder-dynamic">
                                            </i>
                                        </span>
                                    </label>
                                </div>
                                <div class="input-holder">
                                    <div class="input-space">
                                        <input data-bind="value: address1, disable: institutionid" type="text">
                                        </input>
                                    </div>
                                </div>
                                <div class="message-holder">
                                    <small class="" data-bind="css:{ error: address1.hasError, warning: address1.hasWarning }">
                                        <i class="fa fa-holder">
                                        </i>
                                        <!-- ko text: address1.validationMessage -->
                                        <!-- /ko -->
                                    </small>
                                </div>
                            </div>
                            <div class="question column large-4">
                                <div class="label-holder">
                                    <label>
                                        <span class="text">
                                            Street Address 2
                                        </span>
                                        <span class="required-holder">
                                            <i class="fa fa-holder-dynamic">
                                            </i>
                                        </span>
                                    </label>
                                </div>
                                <div class="input-holder">
                                    <div class="input-space">
                                        <input data-bind="value: address2, disable: institutionid" type="text">
                                        </input>
                                    </div>
                                </div>
                                <div class="message-holder">
                                    <small class="error">
                                        <i class="fa fa-holder">
                                        </i>
                                    </small>
                                </div>
                                <div class="hint strong">
                                    (optional)
                                </div>
                            </div>
                            <div class="question column large-4">
                                <div class="label-holder">
                                    <label>
                                        <span class="text">
                                            Street Address 3
                                        </span>
                                        <span class="required-holder">
                                            <i class="fa fa-holder-dynamic">
                                            </i>
                                        </span>
                                    </label>
                                </div>
                                <div class="input-holder">
                                    <div class="input-space">
                                        <input data-bind="value: address2, disable: institutionid" type="text">
                                        </input>
                                    </div>
                                </div>
                                <div class="message-holder">
                                    <small class="error">
                                        <i class="fa fa-holder">
                                        </i>
                                    </small>
                                </div>
                                <div class="hint strong">
                                    (optional)
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="question column large-6 " data-bind="css: { error: city.hasError, required: city.isRequired, warning: city.hasWarning }">
                                <div class="label-holder">
                                    <label>
                                        <span class="text">
                                            City
                                        </span>
                                        <span class="required-holder">
                                            <i class="fa fa-holder-dynamic">
                                            </i>
                                        </span>
                                    </label>
                                </div>
                                <div class="input-holder">
                                    <div class="input-space">
                                        <input data-bind="value: city, disable:institutionid" type="text">
                                        </input>
                                    </div>
                                </div>
                                <div class="message-holder">
                                    <small class="" data-bind="css:{ error: city.hasError, warning: city.hasWarning }">
                                        <i class="fa fa-holder">
                                        </i>
                                        <!-- ko text: city.validationMessage -->
                                        <!-- /ko -->
                                    </small>
                                </div>
                            </div>
                            <div class="question column large-3 small-6 " data-bind="css: { error: statecode.hasError, required: statecode.isRequired, warning: statecode.hasWarning }">
                                <div class="label-holder">
                                    <label>
                                        <span class="text">
                                            State/Province
                                        </span>
                                        <span class="required-holder">
                                            <i class="fa fa-holder-dynamic">
                                            </i>
                                        </span>
                                    </label>
                                </div>
                                <div class="input-holder">
                                    <div class="input-space">
                                        <select class="show" data-bind="css: { 'show': statecode.showText(), 'hide': !statecode.showText() }, options: $root.address().statecodeListHolder, optionsText: 'Text', optionsValue: 'Value', optionsCaption: 'Choose...', value: statecode, disable: institutionid">
                                            <option value="">
                                                Choose...
                                            </option>
                                            <option value="AL">
                                                Alabama
                                            </option>
                                            <option value="AK">
                                                Alaska
                                            </option>
                                            <option value="AZ">
                                                Arizona
                                            </option>
                                            <option value="AR">
                                                Arkansas

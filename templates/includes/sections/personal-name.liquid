<div class="page-wrapper">
    <div class="row">
        <div class="column small-12">
            <h1 class="with-underline">
                Personal Information
                <div class="sub-line">
                    All fields denoted with an
                    <sup>
                        <i class="fa required">
                        </i>
                    </sup>
                    are required.
                </div>
            </h1>
        </div>
    </div>
    <div class="row">
        <div class="question column large-2 medium-3 small-4" data-bind="css: { error: prefix.hasError, required: prefix.isRequired }">
            <div class="label-holder">
                <label>
                    <span class="text">
                        Prefix
                    </span>
                    <span class="required-holder">
                        <i class="fa fa-holder-dynamic">
                        </i>
                    </span>
                </label>
            </div>
            <div class="input-holder">
                <div class="input-space">
                    <select data-bind="options: prefixList, optionsText: 'Text', optionsValue: 'Value', optionsCaption: 'Choose...', value: prefix, disable: true" disabled="">
                        <option value="">
                            Choose...
                        </option>
                        <option value="AMB">
                            Ambassador
                        </option>
                        <option value="ATTY">
                            Attorney
                        </option>
                        <option value="CAPT">
                            Captain
                        </option>
                        <option value="CMDR">
                            Commander
                        </option>
                        <option value="COL">
                            Colonel
                        </option>
                        <option value="CPL">
                            Corporal
                        </option>
                        <option value="DR">
                            Dr.
                        </option>
                        <option value="FR">
                            Father
                        </option>
                        <option value="GEN">
                            General
                        </option>
                        <option value="GOV">
                            Governor
                        </option>
                        <option value="HON">
                            Honorable
                        </option>
                        <option value="LT">
                            Lieutenant
                        </option>
                        <option value="LT COL">
                            Lieutenant Colonel
                        </option>
                        <option value="LT GEN">
                            Lieutenant General
                        </option>
                        <option value="MAJ">
                            Major
                        </option>
                        <option value="MISS">
                            Miss
                        </option>
                        <option value="MR">
                            Mr.
                        </option>
                        <option value="MRS">
                            Mrs.
                        </option>
                        <option value="MS">
                            Ms.
                        </option>
                        <option value="MSGR">
                            Monsignor
                        </option>
                        <option value="OFC">
                            Officer
                        </option>
                        <option value="PRES">
                            President
                        </option>
                        <option value="PROF">
                            Professor
                        </option>
                        <option value="PVT">
                            Private
                        </option>
                        <option value="REP">
                            Representative
                        </option>
                        <option value="REV">
                            Reverend
                        </option>
                        <option value="SEN">
                            Senator
                        </option>
                        <option value="SGT">
                            Sergeant
                        </option>
                        <option value="SIR">
                            Sir
                        </option>
                        <option value="SR">
                            Sister
                        </option>
                    </select>
                    <span class="input-lock" data-bind="visible: true">
                        <i aria-describedby="tooltip-j2q2mq1c0" class="fa fa-lock lock-symbol has-tip" data-selector="tooltip-j2q2mq1c0" data-tooltip="" title="">
                        </i>
                    </span>
                </div>
            </div>
            <div class="message-holder">
                <small class="" data-bind="css:{ error: prefix.hasError, warning: prefix.hasWarning }">
                    <i class="fa fa-holder">
                    </i>
                    <!-- ko text: prefix.validationMessage -->
                    <!-- /ko -->
                </small>
            </div>
        </div>
        <div class="question column large-3 medium-5 small-8 required" data-bind="css: { error: firstname.hasError, required: firstname.isRequired }">
            <div class="label-holder">
                <label>
                    <span class="text">
                        First Name
                    </span>
                    <span class="required-holder">
                        <i class="fa fa-holder-dynamic">
                        </i>
                    </span>
                </label>
            </div>
            <div class="input-holder">
                <div class="input-space">
                    <input data-bind="value: firstname, disable: true" disabled="" type="text">
                        <span class="input-lock" data-bind="visible: true">
                            <i aria-describedby="tooltip-j2q2mq1c1" class="fa fa-lock lock-symbol has-tip" data-selector="tooltip-j2q2mq1c1" data-tooltip="" title="">
                            </i>
                        </span>
                    </input>
                </div>
            </div>
            <div class="message-holder">
                <small class="" data-bind="css:{ error: firstname.hasError, warning: firstname.hasWarning }">
                    <i class="fa fa-holder">
                    </i>
                    <!-- ko text: firstname.validationMessage -->
                    <!-- /ko -->
                </small>
            </div>
        </div>
        <div class="question column large-2 medium-4" data-bind="css: { error: middlename.hasError, required: middlename.isRequired }">
            <div class="label-holder">
                <label>
                    <span class="text swappable large">
                        <span class="long-text">
                            Middle Initial
                        </span>
                        <span class="short-text">
                            MI
                        </span>
                    </span>
                    <span class="required-holder">
                        <i class="fa fa-holder-dynamic">
                        </i>
                    </span>
                </label>
            </div>
            <div class="input-holder">
                <div class="input-space">
                    <input data-bind="value: middlename, disable: true" disabled="" type="text">
                        <span class="input-lock" data-bind="visible: true">
                            <i aria-describedby="tooltip-j2q2mq1c2" class="fa fa-lock lock-symbol has-tip" data-selector="tooltip-j2q2mq1c2" data-tooltip="" title="">
                            </i>
                        </span>
                    </input>
                </div>
            </div>
            <div class="message-holder">
                <small class="" data-bind="css:{ error: middlename.hasError, warning: middlename.hasWarning }">
                    <i class="fa fa-holder">
                    </i>
                    <!-- ko text: middlename.validationMessage -->
                    <!-- /ko -->
                </small>
            </div>
        </div>
        <div class="question column large-3 medium-8 small-8 required" data-bind="css: { error: lastname.hasError, required: lastname.isRequired }">
            <div class="label-holder">
                <label>
                    <span class="text">
                        Last Name
                    </span>
                    <span class="required-holder">
                        <i class="fa fa-holder-dynamic">
                        </i>
                    </span>
                </label>
            </div>
            <div class="input-holder">
                <div class="input-space">
                    <input data-bind="value: lastname, disable: true" disabled="" type="text">
                        <span class="input-lock" data-bind="visible: true">
                            <i aria-describedby="tooltip-j2q2mq1c3" class="fa fa-lock lock-symbol has-tip" data-selector="tooltip-j2q2mq1c3" data-tooltip="" title="">
                            </i>
                        </span>
                    </input>
                </div>
            </div>
            <div class="message-holder">
                <small class="" data-bind="css:{ error: lastname.hasError, warning: lastname.hasWarning }">
                    <i class="fa fa-holder">
                    </i>
                    <!-- ko text: lastname.validationMessage -->
                    <!-- /ko -->
                </small>
            </div>
        </div>
        <div class="question column large-2 medium-4 small-4" data-bind="css: { error: suffix.hasError, required: suffix.isRequired }">
            <div class="label-holder">
                <label>
                    <span class="text">
                        Suffix
                    </span>
                    <span class="required-holder">
                        <i class="fa fa-holder-dynamic">
                        </i>
                    </span>
                </label>
            </div>
            <div class="input-holder">
                <div class="input-space">
                    <select data-bind="options: suffixList, optionsText: 'Text', optionsValue: 'Value', optionsCaption: 'Choose...', value: suffix, disable: true" disabled="">
                        <option value="">
                            Choose...
                        </option>
                        <option value="III">
                            III
                        </option>
                        <option value="JR">
                            Jr.
                        </option>
                        <option value="SR">
                            Sr.
                        </option>
                        <option value="II">
                            II
                        </option>
                        <option value="IV">
                            IV
                        </option>
                    </select>
                    <span class="input-lock" data-bind="visible: true">
                        <i aria-describedby="tooltip-j2q2mq1c4" class="fa fa-lock lock-symbol has-tip" data-selector="tooltip-j2q2mq1c4" data-tooltip="" title="">
                        </i>
                    </span>
                </div>
            </div>
            <div class="message-holder">
                <small class="" data-bind="css:{ error: suffix.hasError, warning: suffix.hasWarning }">
                    <i class="fa fa-holder">
                    </i>
                    <!-- ko text: suffix.validationMessage -->
                    <!-- /ko -->
                </small>
            </div>
        </div>
    </div>
            <div class="row">
            <div class="question column medium-6 required" data-bind="css: { error: designation.hasError, required: designation.isRequired }">
                <div class="label-holder">
                    <label>
                        <span class="text">
                            Credentials
                        </span>
                        <span class="required-holder">
                            <i class="fa fa-holder-dynamic">
                            </i>
                        </span>
                    </label>
                </div>
                <div class="input-holder">
                    <div class="input-space">
                        
                        <input type="text" disabled="true" class="disabled">
                        <span class="input-lock" data-bind="visible: true && true">
                            <i aria-describedby="tooltip-j2q2mq1c5" class="fa fa-lock lock-symbol has-tip" data-selector="tooltip-j2q2mq1c5" data-tooltip="" title="">
                            </i>
                        </span>
                    </div>
                </div>
                <div class="message-holder">
                    <small class="" data-bind="css:{ error: designation.hasError, warning: designation.hasWarning }">
                        <i class="fa fa-holder">
                        </i>
                        <!-- ko text: designation.validationMessage -->
                        <!-- /ko -->
                    </small>
                </div>
            </div>
            <div class="question column medium-6 required" data-bind="css: { error: professionaltypecode.hasError, required: professionaltype.isRequired, warning: professionaltypecode.hasWarning }">
                <div class="label-holder">
                    <label>
                        <span class="text">
                            Professional Type
                        </span>
                        <span class="required-holder">
                            <i class="fa fa-holder-dynamic">
                            </i>
                        </span>
                    </label>
                </div>
                <div class="input-holder row collapse">
                    <div class="input-space column small-3">
                        <button class="button expanded" data-bind="css:{'disabled': false}, disable: false, click: function(){ openModal() },focus: function(){ openModal() }">
                            Add/Edit
                        </button>
                    </div>
                    <div class="input-space column small-9">
                        <input data-bind="value: ( (!isEmpty(professionaltypecategory()) ? professionaltypecategory() : '') + (!isEmpty(professionaltype()) ? ' - ' + professionaltype() : '') + (!isEmpty(professionalsubtype()) ? ' - ' + professionalsubtype() : '') ), disable: true" disabled="" type="text">
                            <input data-bind="value: professionaltypecategory" type="hidden" value="Other Professional">
                                <input data-bind="value: professionaltypecode" type="hidden" value="STUDENT">
                                    <input data-bind="value: professionalsubtypecode" type="hidden" value="PHARMD">
                                        <span class="input-lock" data-bind="visible: (false == true)" style="display: none;">
                                            <i aria-describedby="tooltip-j2q2mq1c6" class="fa fa-lock lock-symbol has-tip" data-selector="tooltip-j2q2mq1c6" data-tooltip="" title="">
                                            </i>
                                        </span>
                                    </input>
                                </input>
                            </input>
                        </input>
                    </div>
                </div>
                <div class="message-holder">
                    <small class="" data-bind="css:{ error: professionaltypecode.hasError, warning: professionaltypecode.hasWarning }">
                        <i class="fa fa-holder">
                        </i>
                        <!-- ko text: professionaltypecode.validationMessage -->
                        <!-- /ko -->
                    </small>
                </div>
            </div>
        </div>
</div>